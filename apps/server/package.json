{
  "name": "server",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "bun run --hot src/index.ts",
    "start": "bun run dist/index.js",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate"
  },
  "dependencies": {
    "@google/genai": "^1.21.0",
    "@neondatabase/serverless": "^1.0.1",
    "@pinecone-database/pinecone": "^6.1.2",
    "better-auth": "^1.3.13",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.2",
    "express": "^5.1.0",
    "langchain": "^0.3.35",
    "multer": "^2.0.2",
    "textract": "^2.5.0",
    "ws": "^8.18.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/bun": "^1.2.6",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.5.2",
    "@types/textract": "^2.4.5",
    "@types/ws": "^8.18.1",
    "drizzle-kit": "^0.31.2",
    "tsdown": "^0.15.1",
    "typescript": "^5.8.2"
  }
}
